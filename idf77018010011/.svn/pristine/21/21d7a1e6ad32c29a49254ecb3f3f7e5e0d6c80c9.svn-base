.asset-securitization-project-title-span{
  display:inline-block;
  vertical-align: middle;
  width:4px;
  height:18px;
  border-radius:2px;
  background-image:linear-gradient(170deg, #77beff, #428fff);
}

.asset-securitization-project-title
{
  font-family: PingFangSC;
  font-size: 14px;
  font-weight: 600;
  font-style: normal;
  font-stretch: normal;
  line-height: normal;
  letter-spacing: normal;
  text-align: left;
  color: #5b5b5b;
  margin-left:6px;
  vertical-align: middle;
}

.asset-securitization-project-table-one
{
  font-family: PingFangSC;
  font-size: 13px;
  font-weight: 600;
  font-style: normal;
  font-stretch: normal;
  line-height: normal;
  letter-spacing: normal;
  text-align: left;
  color: #676767;
}
.asset-securitization-project-table-one>tbody>tr
{
  height: 30px;
}

.asset-securitization-project-table-two-title
{
  padding: 15px 0 0 15px;
  font-family: PingFangSC;
  font-size: 13px;
  font-weight: 600;
  font-style: normal;
  font-stretch: normal;
  line-height: normal;
  letter-spacing: normal;
  text-align: left;
  color: #2e609a;
}

.asset-securitization-project-table-two
{
  width:100%;
  font-family: PingFangSC;
  font-size: 13px;
  font-weight: 600;
  font-style: normal;
  font-stretch: normal;
  line-height: normal;
  letter-spacing: normal;
  text-align: center;
  color: #676767;
  padding: 0 30px 0 40px;
}
.asset-securitization-project-table-two>tbody>tr
{
  height: 30px;
}

.asset-securitization-project-table-two>tbody>tr>td
{
  padding: 8px 0;
}
