.rightone-body{
    margin: 0 5px 0 5px;
    padding: 5px;
    background-color: #ffffff;
    border-top: 0;
}

.rightone-body-right-one-table{
    border-radius: 2px;
    box-shadow: 0 2px 4px 0 rgba(214, 214, 214, 0.5);
    width: 100%;
}

.rightone-body-right-one-text{
    height: 18px;
    font-family: PingFangSC;
    font-size: 13px;
    font-weight: 600;
    font-style: normal;
    font-stretch: normal;
    line-height: normal;
    letter-spacing: normal;
    text-align: left;
    color: #4e4e4e;
    padding-left:4px;
}

.rightone-body-right-one-table-head{
    box-shadow: 0 2px 4px 0 rgba(214, 214, 214, 0.5);
    background-image: linear-gradient(126deg, #77beff, #428fff);
    border-radius: 3px 3px 0 0;
    height: 16px;
    font-family: PingFangSC;
    font-size: 11px;
    font-weight: 600;
    font-style: normal;
    font-stretch: normal;
    line-height: normal;
    letter-spacing: normal;
    text-align: left;
    color: #ffffff;
}

.rightone-body-right-one-table-body{
    height: 16px;
    font-family: PingFangSC;
    font-size: 11px;
    font-weight: normal;
    font-style: normal;
    font-stretch: normal;
    line-height: normal;
    letter-spacing: normal;
    text-align: center;
    color: #858585;  
}

