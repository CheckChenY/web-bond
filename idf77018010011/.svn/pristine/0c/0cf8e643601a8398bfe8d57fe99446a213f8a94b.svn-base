} */

.returnpassword-all .ant-btn-primary{
    width: 300px;
}

.previous{
    width: 300px;
    margin-top: 10px;
    border: 0;
    font-family: PingFangSC;
    font-size: 13px;
    font-weight: 600;
    font-style: normal;
    font-stretch: normal;
    line-height: normal;
    letter-spacing: normal;
    text-align: center;
    color: #4390ff;
}

.get-safty{
    width: 113px;
    height: 35px;
    border-radius: 4px;
    box-shadow: 0 2px 4px 0 #c2d9fb;
    background-image: linear-gradient(112deg, #77beff, #428fff);
    font-family: PingFangSC;
    font-size: 13px;
    font-weight: 600;
    font-style: normal;
    font-stretch: normal;
    line-height: normal;
    letter-spacing: normal;
    text-align: center;
    color: #ffffff;
}

.ensure-change{
    width: 300px;
    height: 35px;
    border-radius: 4px;
    box-shadow: 0 2px 4px 0 #c2d9fb;
    background-image: linear-gradient(112deg, #77beff, #428fff);
    font-family: PingFangSC;
    font-size: 13px;
    font-weight: 600;
    font-style: normal;
    font-stretch: normal;
    line-height: normal;
    letter-spacing: normal;
    text-align: center;
    color: #ffffff;  
}

.change-succeed{
    width: 300px;
    height: 35px;
    border-radius: 4px;
    box-shadow: 0 2px 4px 0 #c2d9fb;
    background-image: linear-gradient(112deg, #77beff, #428fff);
    font-family: PingFangSC;
    font-size: 13px;
    font-weight: 600;
    font-style: normal;
    font-stretch: normal;
    line-height: normal;
    letter-spacing: normal;
    text-align: center;
    color: #ffffff; 
}

.email{
    margin-bottom:20px;
}

.email-all{
    width:300px;
    border:0;
    text-align:left;
    display:inline-block;
}

.email-name{
    padding:5px;
    height: 18px;
    font-family: PingFangSC;
    font-size: 13px;
    font-weight: normal;
    font-style: normal;
    font-stretch: normal;
    line-height: normal;
    letter-spacing: normal;
    text-align: center;
    color: #666666;
}

.email-number{
    padding:4px;
    height: 18px;
    font-family: PingFangSC;
    font-size: 13px;
    font-weight: normal;
    font-style: normal;
