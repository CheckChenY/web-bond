.bondsearchleft{
    margin: 0 5px 0 5px;
    border: 1px solid #e0e0e0;
    padding: 5px;
    /* padding: 10px; */
    background-color: #ffffff;
    box-shadow: 0 2px 4px 0 rgba(178, 178, 178, 0.5);
}

.bondsearchleft-header{
    padding: 5px 0 5px 0;
    margin-bottom: 10px;
    background-color: #fafafa;
}

.bondsearchleft-header-solid{
    display: inline-block;
    border-left: 5px solid #d8d8d8;
    height: 20px;
    line-height: 20px;
    font-family: PingFangSC;
    font-size: 14px;
    font-weight: normal;
    font-style: normal;
    font-stretch: normal;
    letter-spacing: normal;
    text-align: center;
    color: #5f5f5f;
}

.bondsearch-title-one{
    border-bottom: solid 1px #ebebeb;
    padding-bottom: 10px;
    text-align: center;
}

.bondsearch-title-name{
    font-family: PingFangSC;
    font-size: 13px;
    color: #4e4e4e;
    text-align: left;
    margin-bottom: 10px;
}

